{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "15mb",
          "includeFiles": [
            "uploads/**",
            "client/build/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      },
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ]
  }